generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("NEXT_PUBLIC_MONGODB_URI")
}

type LastRaceCircuit {
  id    Int
  image String
  name  String
}

type LastRaceCompetition {
  id       Int
  location LastRaceCompetitionLocation
  name     String
}

type LastRaceCompetitionLocation {
  city    String
  country String
}

type LastRaceFastestLap {
  driver LastRaceFastestLapDriver
  time   String
}

type LastRaceFastestLapDriver {
  id Int
}

type LastRaceLaps {
  /// Could not determine type: the field only had null or empty values in the sample set.
  current Int?
  total   Int
}

type LastWinnerDriver {
  abbr   String
  id     Int
  image  String
  name   String
  number Int
}

type LastWinnerRace {
  id Int
}

type LastWinnerTeam {
  id   Int
  logo String
  name String
}

type RankingDriversDriver {
  abbr   String?
  id     Int
  image  String
  name   String
  number Int?
}

type RankingDriversTeam {
  id   Int
  logo String
  name String
}

model lastRace {
  id          String              @id @default(auto()) @map("_id") @db.ObjectId
  circuit     LastRaceCircuit
  competition LastRaceCompetition
  date        String
  distance    String
  fastest_lap LastRaceFastestLap
  id_         Int                 @map("id")
  laps        LastRaceLaps
  season      Int
  status      String
  timezone    String
  type        String
  updated  DateTime?         @db.Date
  /// Could not determine type: the field only had null or empty values in the sample set.
  weather     Json?
}

model lastWinner {
  id       String           @id @default(auto()) @map("_id") @db.ObjectId
  driver   LastWinnerDriver
  /// Could not determine type: the field only had null or empty values in the sample set.
  gap      Int?
  grid     String
  laps     Int
  pits     Int
  position Int
  race     LastWinnerRace
  team     LastWinnerTeam
  time     String
  updated  DateTime?         @db.Date
}

model rankingDrivers {
  id       String               @id @default(auto()) @map("_id") @db.ObjectId
  /// Could not determine type: the field only had null or empty values in the sample set.
  pilots   RankingDriversPilots[]
  season   Int
  updated  DateTime?         @db.Date
}

type RankingDriversPilots {
  behind   Json?
  driver   RankingDriversDriver
  points   Int?
  position Int
  season   Int
  team     RankingDriversTeam
  /// Could not determine type: the field only had null or empty values in the sample set.
  wins     Json?
}